{
    "name": "bitban/img-dimensions",
    "description": "Librería que permite calcular las dimensiones de imágenes al vuelo",
    "minimum-stability": "stable",
    "license": "MIT",
    "keywords": [
        "html"
    ],
    "type": "library",
    "authors": [
        {
            "name": "Bitban Technologies",
            "email": "info@bitban.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4":{
            "Bitban\\Utils\\ImgDimensions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bitban\\Utils\\ImgDimensions\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin/",
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        }
    }
}
