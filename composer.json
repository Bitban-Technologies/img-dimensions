{
    "name": "bitban/img-dimensions",
    "description": "Librería que permite calcular las dimensiones de imágenes al vuelo",
    "minimum-stability": "stable",
    "license": "proprietary",
    "type": "library",
    "authors": [
        {
            "name": "Bitban Technologies",
            "email": "info@bitban.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "bitban/php-code-quality-tools": "dev-master",
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4":{
            "Bitban\\Utils\\ImgDimensions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bitban\\Utils\\ImgDimensions\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin/",
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Bitban\\PhpCodeQualityTools\\Composer\\ScriptHandler::checkHooks"
        ],
        "post-update-cmd": [
            "Bitban\\PhpCodeQualityTools\\Composer\\ScriptHandler::checkHooks"
        ]
    }
}
